extends javascript, css

snippet template "HTML Template"
<!doctype html>
<html>
	<head>
	<meta charset="utf-8">
		$1
	</head>
	<body>
		$2
	</body>
</html>
endsnippet

snippet el "generic element" b
<$1>
	$2
</$1>
endsnippet

priority 1
snippet optel "generic element with attributes" b
<$1 $2>
	$3
</$1>
endsnippet

snippet oel "one line element" w
<$1>$2</$1>
endsnippet

priority 1
snippet optoel "one line element with attributes" w
<$1 $2>$3</$1>$4
endsnippet
